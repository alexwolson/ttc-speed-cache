# Vercel Blob Storage (required for automated collection)
# Create a Blob Store in your Vercel project and copy the token from Environment Variables
BLOB_READ_WRITE_TOKEN=your_vercel_blob_token_here

# Optional: Cron authentication
# Generate a random secret to protect the /api/collect-sample endpoint
CRON_SECRET=your_random_secret_here

# Upstash QStash (for per-minute scheduler)
# Get token and signing keys from Upstash console
QSTASH_TOKEN=your_upstash_qstash_token_here
QSTASH_CURRENT_SIGNING_KEY=your_qstash_current_signing_key
QSTASH_NEXT_SIGNING_KEY=your_qstash_next_signing_key


